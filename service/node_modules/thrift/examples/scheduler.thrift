namespace php scheduler

struct Job{
    1: optional i32 id,
    2: optional string type,
    3: optional i32 increment,
    4: optional string params,
    5: optional string period,
    6: optional string last_run,
    7: optional string next_run
}

service JobExchange{
    i32 insert_job(1: Job j),
    Job get_job(1: i32 jid),
    set<Job> get_all()
}
